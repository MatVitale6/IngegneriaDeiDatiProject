            [
                "<th class=\"ltx_td ltx_align_left ltx_th ltx_th_row ltx_border_b ltx_border_l ltx_border_r ltx_border_t\" id=\"S4.T1.1.1.4.2.1\">\n          GPT-4\n         </th>\n         \n",
